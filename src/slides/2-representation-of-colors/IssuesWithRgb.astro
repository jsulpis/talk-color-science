---
import hsb1 from "./assets/hsb-circle/Group 1.png";
import hsb2 from "./assets/hsb-circle/Group 2.png";
import hsb3 from "./assets/hsb-circle/Group 3.png";
import hsb4 from "./assets/hsb-circle/Group 4.png";
import srgb from "../0-intro/assets/srgb.png";
import ImageStack from "../../components/ImageStack.astro";

const hsbImages = [srgb, hsb1, hsb2, hsb3, hsb4].map(({ src }) => src);
---

<ImageStack images={hsbImages} class="stack" />

<style>
	.stack {
		margin-top: -5%;
	}
</style>
